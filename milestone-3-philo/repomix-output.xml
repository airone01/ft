#include "philo.h"

/**
 * @brief Main entry to the program
 *
 * @param argc Arguments count
 * @param argv Arguments
 * @return int Return status
 */
int	main(int argc, char **argv)
{
	int		code;
	t_ctx	*ctx;
	t_philo	*philos;

	if (args(argc, argv))
